@MAINTAINER_MODE_TRUE@unrename:
@MAINTAINER_MODE_TRUE@	for file in *.c *.h ; do \
@MAINTAINER_MODE_TRUE@	  case "$$file" in \
@MAINTAINER_MODE_TRUE@	  io*|bit_slicer*|decoder.c|raw_decoder*|sampling_par.c) \
@MAINTAINER_MODE_TRUE@	    continue ; ;; \
@MAINTAINER_MODE_TRUE@	  esac ; \
@MAINTAINER_MODE_TRUE@	  if grep -q -i vbi3_ "$$file" ; then \
@MAINTAINER_MODE_TRUE@	    sed 's/vbi3_/vbi_/g;s/VBI3_/VBI_/g' < "$$file" >tmp ; \
@MAINTAINER_MODE_TRUE@	    mv tmp "$$file" ; \
@MAINTAINER_MODE_TRUE@	  fi ; \
@MAINTAINER_MODE_TRUE@	done
